pipeline {
environment {
registry = "14123/ubuntu-modified"
registryCredential = 'DockerHub'
dockerImage = ''
}
agent any
stages {
stage('Cloning our Git') {
steps {
git 'https://github.com/vltdxchange/pipeline.git'
}
}
stage('Building our image') {
steps{
script {
  
docker login https://hub.docker.com/repository/docker/14123
docker pull https://hub.docker.com/repository/docker/14123/ubuntu-modified
}
}
}

}
